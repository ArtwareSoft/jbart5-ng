    "es6-shim": "^0.35.0",
    "history": "^2.0.1",
    "material-design-lite": "^1.3.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.1",
    "systemjs": "^0.19.27",
    "typescript": "^2.0.2",
    "zone.js": "0.8.4"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.34",
